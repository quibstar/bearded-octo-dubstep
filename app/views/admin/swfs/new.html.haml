#form-remote
  =form_for [:admin, @swf] do |f|
    .modal-header
      =link_to content_tag(:i, '',:class => "fontello-icon-cancel" ),'', :class=> "close close-form", 'aria-hidden' => "true"
      %h4
        New SWF File
    .modal-body
      -if @swf
        =render 'shared/error_messages', :object => @swf 
      =render "admin/swfs/form_fields", :f => f
    .modal-footer
      =f.submit "Save", :class =>'btn btn-primary'