#form-remote
  =form_for [:admin, @multi_navigation] do |f|
    .modal-header
      =link_to content_tag(:i, '',:class => "fontello-icon-cancel" ),'', :class=> "close close-form", 'aria-hidden' => "true"
      %h4
        New Multi-navigations
    .modal-body
      -if @multi_navigation
        =render 'shared/error_messages', :object => @multi_navigation 
      =render "admin/multi_navigations/form_fields", :f => f
    .modal-footer
      =f.submit "Save", :class =>'btn btn-primary'