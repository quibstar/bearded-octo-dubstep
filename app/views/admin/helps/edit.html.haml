#form-remote
  =form_for [:admin, @help], :html => {:id => "edit_youtube"} do |f|
    .modal-header
      =link_to content_tag(:i, '',:class => "fontello-icon-cancel" ),'', :class=> "close close-form", 'aria-hidden' => "true"
      %h4
        =@help.title
    .modal-body
      -if @help
        =render 'shared/error_messages', :object => @help 
      =render "admin/helps/form_fields", :f => f
    .modal-footer
      =f.submit "Save", :class =>'btn btn-primary'